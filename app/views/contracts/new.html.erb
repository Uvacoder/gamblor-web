<%= form_with model: @contract, url: '/contracts' do |f| %>
  <%= f.label :player_id %>
  <%= f.select :player_id, @players %> 

  <%= f.label :team_id %>
  <%= f.select :team_id, @teams %> 

  <%= f.label :start %>
  <%= f.date_field :start %>

  <%= f.label :end %>
  <%= f.date_field :end %>

  <%= f.submit %>
<% end %>
